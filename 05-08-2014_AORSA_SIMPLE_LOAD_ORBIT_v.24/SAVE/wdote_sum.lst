(  810)         b = aimag(zin)
(  811) 
(  812) !       z = (a + i * b)
(  813) !       1/z =  a/(a^2 + b^2) - i * b/(a^2 + b^2)
(  814) !
(  815) !       or    1/(a + (b/a)*b) - i * (b/a) / (a + (b/a)*b)
(  816) !       or    (a/b)/( (a/b)*a + b ) - i * 1/( (a/b)*a + b )
(  817) !        
(  818)         if (abs(a).gt.abs(b)) then
(  819)             b_over_a = b/a
(  820)             d = a + (b_over_a)*b
(  821)             rd = one/d
(  822)             zout = cmplx( rd, -(b_over_a)*rd )
(  823)         else
(  824)             a_over_b = a/b
(  825)             d = (a_over_b)*a + b
(  826)             rd = one/d
(  827)             zout = cmplx( (a_over_b)*rd, -rd )
(  828)         endif
